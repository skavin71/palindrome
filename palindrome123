import java.util.*;
public class Main{
    public static void main(String[] args){
    int r=0,temp,sum=0;
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    temp=n;
        while(n>0){
        r=n%10;//geting reminder
        sum=(sum*10)+r;
        n=n/10;
        }
    
    if(temp==sum)
        System.out.println("Yes");
    else
        System.out.println("No");
    }
}  
